    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="template/Default/js/lightslider.js"></script>
    <script src="template/Default/js/main.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            $(window).scroll( function(){
                $('.hideme').each( function(i){
                    var bottom_of_object = $(this).offset().top + $(this).outerHeight();
                    var bottom_of_window = $(window).scrollTop() + $(window).height();
                    if( bottom_of_window > bottom_of_object ){
                        $(this).animate({'opacity':'1'},500);
                        $(this).addClass("w3-animate-right");
                    }
                }); 
            });
            /*----------------------------------*/
            /*----------------------------------*/
            var rong = $(window).width();
            if (rong <= 991) {
                $(".block-main-dv").removeClass("hideme");
            }
            /*----------------------------------*/
			var inputData=[];
			inputData["sizewidth"]=document.getElementById("sizewidth").value;
			inputData["sizeheight"]=document.getElementById("sizeheight").value;
			inputData["soluong"]=document.getElementById("soluong").value;
			inputData["decaltype"]=document.getElementById("decaltype").value;
			inputData["isextrusion"]=document.getElementById("isextrusion").value;
			var arrDulieu = $("#inputData").serialize();
			$( "#sizewidth" ).change(function() {
				inputData["sizewidth"]=document.getElementById("sizewidth").value;
				arrDulieu = $("#inputData").serialize();
				checkvalid(arrDulieu,inputData);
			});
			$( "#sizeheight" ).change(function() {
				inputData["sizeheight"]=document.getElementById("sizeheight").value;
				arrDulieu = $("#inputData").serialize();
				checkvalid(arrDulieu,inputData);
			});
			$( "#soluong" ).change(function() {
				inputData["soluong"]=document.getElementById("soluong").value;
				arrDulieu = $("#inputData").serialize();
				checkvalid(arrDulieu,inputData);
			});
			$( "#decaltype" ).change(function() {
				inputData["decaltype"]=document.getElementById("decaltype").value;
				arrDulieu = $("#inputData").serialize();
				checkvalid(arrDulieu,inputData);
			});
			$( "#isextrusion" ).change(function() {
				inputData["isextrusion"]=document.getElementById("isextrusion").value;
				arrDulieu = $("#inputData").serialize();
				checkvalid(arrDulieu,inputData);
			});
        });
		function checkvalid(arrDulieu,inputData){
			$.ajax({
				type:'post',
				url:'ajax',
				data:{'cmd':'validInput', arrDulieu:arrDulieu } ,
				success:function(data){
					if(data.result==true){
						$("#resultPrice").css("display", "block");
						/*tính toán*/
						/*số con / 1 tờ in*/
						var socontrento= Math.floor(280/inputData["sizewidth"])*Math.floor(280/inputData["sizeheight"]);
						/*số tờ cần in*/
						var sotocanin=Math.ceil(inputData["soluong"]/socontrento)+3;
						<!-- alert('Số lượng '+inputData["soluong"]); -->
						<!-- alert('Số tờ cần in'+sotocanin); -->
						var dongia=10000/socontrento;
						<!-- alert("đơn giá"+dongia); -->
						var thanhtien=dongia*inputData["soluong"];
						<!-- alert("Thành tiền"+thanhtien); -->
						dongia=parseInt(Math.ceil(dongia)).toLocaleString();
						document.getElementById("resultDongia").innerHTML=dongia+" VNĐ";
						thanhtien=parseInt(thanhtien).toLocaleString();
						document.getElementById("resulTien").innerHTML=thanhtien+" VNĐ";
						
						document.getElementById("resultsize").innerHTML=inputData["sizewidth"]+'x'+inputData["sizeheight"]+'mm';
						document.getElementById("resultsoluong").innerHTML=inputData["soluong"];
						document.getElementById("resultLoaidecal").innerHTML=data.loaidecal;
						
					}else{
						$("#resultPrice").css("display", "none");
					}
				}
			});
		}
    </script>
	<!-- {thongtincongty.tawk } -->
